{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "command": "gulp",
  "args": [
    "--no-color"
  ],
  "tasks": [
    {
      "label": "build-sourcemaps",
      "args": [ "build-sourcemaps" ],
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$lessCompile",
        "$tsc",
        "$jshint"
      ]
    }
  ]
}
